{
    "name": "rsislocal",
    "version": "0.1.9",
    "description": "standalone rsis (R Stock-In System) (on MERN)",
    "private": true,
    "homepage": ".",
    "directories": {
        "lib": "/src/lib"
    },
    "scripts": {
        "production": "NODE_ENV=production npm run start",
        "/1 - for Heroku/": "*******   for Heroku    *******",
        "start": "node server/www.js",
        "build": "node scripts/bundle-builder.js",
        "/2 - bundle building & start/": "",
        "start:prod": "NODE_ENV=production npm run start",
        "bb:prod": "node scripts/bundle-builder.js",
        "bb:dev": "node scripts/bundle-builder.js --development-config --stats",
        "/3 - HMR Debug/": "    *** for hot module replacement development ***",
        "hmr:start": "DEV_MODE=HotModuleReplacement npm run start",
        "hmr:debug": "DEV_MODE=HotModuleReplacement npm run debug",
        "hmr:all": "DEV_MODE=HotModuleReplacement npm run debug:all",
        "debug": "git branch && DEBUG=api:*,dbs:* npm run start",
        "debugger": "git branch && DEBUG=reports:* npm run start",
        "debug:all": "DEBUG=* npm run start",
        "/4 - webpack builder/": "     *** for 'static' build development ***",
        "build:wp:prod": "webpack --config config/webpack.prod.js",
        "build:wp:dev": "webpack --config config/webpack.dev.js",
        "/5 - Old CRA building/": "***  Create React App Scripts  ***",
        "build:prod": "node scripts/build.js",
        "bust:prod": "NODE_ENV=production npm run build && npm run start:prod",
        "build:dev": "node scripts/devbuild.js --production-config",
        "bust:dev": "npm run build:dev && npm run start",
        "build:devhmr": "node scripts/devbuild.js --development-config",
        "bust:devhmr": "npm run build:devhmr && npm run start",
        "/6 - /": "",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
        "forever": "forever start --fifo --minUptime 1000 --spinSleepTime 1000 --killSignal=SIGINT static/server.bundle.js && forever list",
        "/7 -/": "                               ***  for testing ***",
        "jest:srv": "jest server/**/*",
        "jest:dbs": "jest server/databases/**/* --detectOpenHandles",
        "jest:src": "jest src/**",
        "test": "npm run test:lib",
        "test:lib": "mocha \"./src/lib/**/*.spec.js\"",
        "test:rsis": "mocha \"./src/lib/rsis/*.spec.js\"",
        "test:work": "mocha \"./src/lib/**/work*.spec.js\""
    },
    "author": "asklim",
    "license": "ISC",
    "keywords": [
        "rsis",
        "heroku",
        "local app"
    ],
    "engines": {
        "node": "14.x",
        "npm": "7.x"
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
