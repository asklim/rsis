        "jest": "^27.0.6",
        "webpack-dev-middleware": "^4.0.2",
        "webpack-hot-middleware": "^2.25.0"
    },
    "nodemonConfig": {
        "delay": "2000",
        "verbose": true,
        "execMap": {
            "js": "node --experimental-modules",
            "mjs": "node --experimental-modules"
        },
        "ext": "js,json,mjs",
        "watch": [
            "imports/**",
            "server/**",
            "api/**",
            "bin/*"
        ],
        "ignore": [
            ".git",
            ".vscode",
            "public/**",
            "node_modules/*",
            "node_modules/**/*",
            "node_modules/*/**",
            "node_modules/**",
            "src/**",
            "static/**"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/asklim/rsis.git"
    }
}
