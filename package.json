        "asklim": "git+https://asklim@github.com/asklim/asklim.git",
        "autoprefixer": "^10.4.7",
        "axios": "^0.27.2",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-named-asset-import": "^0.3.7",
        "bfj": "^7.0.2",
        "body-parser": "^1.19.0",
        "buffer": "^6.0.3",
