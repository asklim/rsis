        "css-loader": "^6.2.0",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "dayjs": "^1.11.2",
        "debug": "^4.3.1",
        "dotenv": "^16.0.1",
        "dotenv-expand": "^8.0.3",
