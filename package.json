        "@types/react": "^17.0.19",
        "asklim": "git+https://asklim@github.com/asklim/asklim.git",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-named-asset-import": "^0.3.7",
