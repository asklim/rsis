        "@babel/eslint-parser": "^7.17.0",
        "@babel/preset-typescript": "^7.17.12",
        "@emotion/eslint-plugin": "^11.7.0",
        "eslint": "^8.21.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-webpack-plugin": "^3.1.1",
        "jest": "^28.1.0",
        "webpack-dev-middleware": "^5.3.3",
        "webpack-hot-middleware": "^2.25.1"
    },
    "nodemonConfig": {
        "delay": "2000",
        "verbose": true,
        "execMap": {
            "js": "node --experimental-modules",
            "mjs": "node --experimental-modules"
        },
        "ext": "js,json,mjs",
        "watch": [
            "imports/**",
            "server/**",
            "api/**",
            "bin/*"
        ],
        "ignore": [
            ".git",
            ".vscode",
            "public/**",
            "node_modules/*",
            "node_modules/**/*",
            "node_modules/*/**",
            "node_modules/**",
            "src/**",
            "static/**"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/asklim/rsis.git"
    }
}
